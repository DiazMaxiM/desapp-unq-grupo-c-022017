<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="model.TimeZone">
		<id column="ID" name="id">
			<generator class="native" />
		</id>
		<property name="startHours" type="org.jadira.usertype.dateandtime.joda.PersistentLocalDateTime"  />
		<property name="endHours" type="org.jadira.usertype.dateandtime.joda.PersistentLocalDateTime"  />
	</class>
</hibernate-mapping>