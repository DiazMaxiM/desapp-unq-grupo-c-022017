<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="model.User">
		<id column="idAddress" name="id">
			<generator class="foreign" >
 			<param name="property">address</param>
 		</generator>		
 		</id>
		<one-to-one name="telephone" cascade="all" class="model.Telephone" />
		<one-to-one name="address" cascade="all" class="model.Address" />
		
		<property name="cuit" unique="true" />
		<property name="name" />
		<property name="mail" unique="true"/>
		<property name="surname" />
		<property name="password" />
		
	</class>
</hibernate-mapping>